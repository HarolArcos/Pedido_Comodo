<p style="color: green"><%= notice %></p>

<%= render @vendedor %>

<div>
  <%= link_to "Edit this vendedor", edit_vendedor_path(@vendedor) %> |
  <%= link_to "Back to vendedors", vendedors_path %>

  <%= form_tag({:action => :upload}, :multipart => true) do %>
    <%= file_field_tag 'picture' %>
  <% end %>
    

  <%= button_to "Destroy this vendedor", @vendedor, method: :delete %>
</div>
